.history-list {
  display: flex;
  flex-direction: column;
  padding: 40rpx;
}

.header {
  display: flex;
  flex-direction: row;
}

.title {
  flex: 1;
  font-size: 30rpx;
  color: #535c64;
}

.deleteAll {
  font-size: 24rpx;
  color: #8995a1;
}

.item {
  border-top: 1px solid #ececec;
  margin-top: 24rpx;
  padding-top: 24rpx;
}

.item .query {
  color: #8995a1;
}

.item .result {
  margin-top: 16rpx;
}
